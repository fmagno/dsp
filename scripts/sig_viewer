#!/usr/bin/env python
""" """
import platform
if platform.system() == 'Darwin':
    import matplotlib
    matplotlib.use('TkAgg')

import numpy as np
import matplotlib.pyplot as plt
import sys


fs = float(sys.argv[1].split('r')[-1].split('_')[0])

for arg in sys.argv[1:]:

    f = open(arg, 'rb')
    data = np.fromfile(f, dtype='float32')

    t = np.arange(len(data))/fs


    plt.plot(t, data)
plt.show()
